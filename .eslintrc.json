{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "prettier/prettier": "error",
        "no-param-reassign": "off",
        "func-names": "off"
    }
}
